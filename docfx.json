{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "unified-latest",
        "dest": "api/overview/azure",
        "dest": "api",
        "version": "unified-latest",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "overview/**"
        ]
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "unified-latest/overview",
        "dest": "api/overview/azure",
        "dest": "api",
        "version": "unified-latest",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "overview/**"
        ]
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "breadcrumb",
        "dest": "breadcrumb",
        "version": "unified-latest",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "unified-latest",
        "version": "unified-latest",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "docs-ref-autogen"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "src": "unified-latest",
        "version": "unified-latest",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "~/breadcrumb/toc.yml"
    },    
    "markdownEngineName": "markdig",
    "fileMetadata": {},
    "template": [],
    "dest": "_site"
  },
  "langs": {
    "unified-latest/**.yml": [
      "java"
    ],
    "unified-preview/**.yml": [
      "java"
    ]
  }
}
