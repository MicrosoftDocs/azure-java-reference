{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-autogen",
        "dest": "api",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "signalr"
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "breadcrumb",
        "dest": "signalrbreadcrumb",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "signalr"
      },
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "docs-ref-autogen"
        ],
        "group": "signalr"
      }
    ],
    "groups": {
      "signalr": {
        "dest": "signalR",
        "moniker_range": "aspnet-signalr-java"
      }
    },
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/signalrbreadcrumb/toc.json"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "_site"
  }
}
